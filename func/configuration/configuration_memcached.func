#!/bin/bash

# Configuration services memcached function
configuration_memcached()
{
    local module="memcached"
    local version="${memcached_version}"

    [ ! -h "${install_dir_prefix}/${module}" ] && cd "${install_dir_prefix}" && ln -sf "${module}"-"${version}" "${module}"
    ln -sf "${install_dir_prefix}"/"${module}"/bin/memcached /usr/local/bin/memcached
}
