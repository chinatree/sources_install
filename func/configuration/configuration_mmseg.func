#!/bin/bash

# Configuration services mmseg function
configuration_mmseg()
{
    local module="mmseg"
    local version="${mmseg_version}"

    [ ! -h "${install_dir_prefix}/${module}" ] && cd "${install_dir_prefix}" && ln -sf "${module}"-"${version}" "${module}"
    ln -sf "${install_dir_prefix}"/"${module}"/bin/mmseg /usr/local/bin/mmseg
}
