#!/bin/bash

# Configuration php ext of function
configuration_php_ext_FastDFS()
{
    local module="FastDFS"
    local version="${php_ext_FastDFS_version}"

    [ -f "${etc_dir}/${module}/fastdfs_client.ini" ] && cp -a "${etc_dir}/${module}/fastdfs_client.ini" "${install_dir_prefix}/php-${php_version}/etc/extends/fastdfs_client.ini"
}
